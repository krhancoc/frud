{
  "context": {
    "version": "0.0.1",
    "port": 8080
  },
  "database": {
    "type": "neo4j",
    "hostname": "localhost",
    "port": 7687,
    "user": "neo4j",
    "password": "secret"
  },
  "manager": {
    "generics": ["healthcheck"],
    "plugins": [
      {
        "name": "sub",
        "path": "/sub",
        "description": "Tests multiple subfields",
        "model": [
          {
            "key": "name",
            "value_type": "string",
            "options": ["id"]
          },
          {
            "key": "otherfield",
            "value_type": [
              {
                "key": "person",
                "value_type": "people"
              },
              {
                "key": "sub1a",
                "value_type": "string"
              }
            ]

          }
        ]
      },
      {
        "name": "people",
        "path": "/people",
        "description": "People only endpoint",
        "model": [
          {
            "key": "name",
            "value_type": "string",
            "options": ["id"]
          },
          {
            "key": "nickname",
            "value_type": "string"
          },
          {
            "key": "supervisor",
            "value_type": "people"
          },
          {
            "key": "partner",
            "value_type": "people"
          }
        ]
      },
      {
        "name": "meeting",
        "path": "/meeting",
        "description": "Meeting only endpoint",
        "model": [
          {
            "key": "date",
            "value_type": "string",
            "options": ["id"]
          },
          {
            "key": "attending",
            "value_type": "people"
          }
        ]
      }
    ]
  }
}